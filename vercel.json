{
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": "public",
  "framework": "static",
  "env": [
    {
      "key": "FIREBASE_CONFIG",
      "value": "@firebase-config"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
